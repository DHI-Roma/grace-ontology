###  https://w3id.org/grace/ontology/affiliated_to_institution
:affiliated_to_institution rdf:type owl:ObjectProperty ;
                           rdfs:subPropertyOf :pertains_to ;
                           rdfs:domain [ rdf:type owl:Class ;
                                         owl:unionOf ( :biographical_event
                                                       :person
                                                     )
                                       ] ;
                           rdfs:range :institution ;
                           rdfs:label "affiliated to institution"@en .


###  https://w3id.org/grace/ontology/archive_object
:archive_object rdf:type owl:ObjectProperty ;
                rdfs:subPropertyOf :refers_to ;
                rdfs:domain [ rdf:type owl:Class ;
                              owl:unionOf ( :anything
                                            :source
                                          )
                            ] ;
                rdfs:range :archive ;
                rdfs:label "archive object"@en .


###  https://w3id.org/grace/ontology/authority_by_institution
:authority_by_institution rdf:type owl:ObjectProperty ;
                          rdfs:subPropertyOf :in_capacity_of ;
                          rdfs:domain :event ;
                          rdfs:range :institution ;
                          rdfs:comment "an institution being in charge or acting as authority" ;
                          rdfs:label "authority by institution"@en .


###  https://w3id.org/grace/ontology/benefice_category
:benefice_category rdf:type owl:ObjectProperty ;
                   rdfs:subPropertyOf :type_assigned ;
                   rdfs:domain :object ;
                   rdfs:range :type ;
                   rdfs:comment "A classification used to describe the type or status of a church benefice, reflecting its importance, revenue size, or administrative role within the church hierarchy." ;
                   rdfs:label "benefice category"@en .


###  https://w3id.org/grace/ontology/benefice_taxation
:benefice_taxation rdf:type owl:ObjectProperty ;
                   rdfs:subPropertyOf :has_monetary_value ;
                   rdfs:domain :event ;
                   rdfs:range :monetary_value ;
                   rdfs:comment "The assessment or assignment of financial value to a benefice, indicating the income or revenue it generates, typically for taxation or financial management purposes within the church." ;
                   rdfs:label "benefice taxation"@en .


###  https://w3id.org/grace/ontology/benefice_title
:benefice_title rdf:type owl:ObjectProperty ;
                rdfs:subPropertyOf :dedicated_to ;
                rdfs:domain [ rdf:type owl:Class ;
                              owl:unionOf ( :institution
                                            :object
                                          )
                            ] ;
                rdfs:range :dedication ;
                rdfs:comment "A specific ecclesiastical name or title dedicated to a benefice, often reflecting its patron saint, founding figure, or primary function within the church community." ;
                rdfs:label "benefice title"@en .


