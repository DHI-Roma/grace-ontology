grace:appellation_literal a owl:DatatypeProperty ;
    rdfs:label "appellation literal"@en ;
    rdfs:domain grace:anything ;
    rdfs:range rdfs:Literal .

grace:has_participant a owl:ObjectProperty ;
    rdfs:label "has participant"@en ;
    rdfs:domain grace:event ;
    rdfs:range grace:person ;
    rdfs:subPropertyOf grace:person_object .

grace:entry a owl:Class ;
    rdfs:label "entry"@en ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:cardinality "1"^^xsd:nonNegativeInteger ;
            owl:onProperty grace:type_of_entry ],
        grace:anything .

grace:seat_of_worship a owl:Class ;
    rdfs:label "seat of worship"@en ;
    rdfs:comment "auto-generated from erectChurch()" ;
    rdfs:subClassOf grace:institution .

grace:date a owl:Class ;
    rdfs:label "date"@en ;
    rdfs:subClassOf grace:anything .

grace:dedication a owl:Class ;
    rdfs:label "dedication"@en ;
    rdfs:comment "to be discussed" ;
    rdfs:subClassOf grace:anything .

grace:refers_to a owl:ObjectProperty ;
    rdfs:label "refers to"@en ;
    rdfs:comment "A wildcard property to find any direct entity relation" ;
    rdfs:domain grace:anything ;
    rdfs:range grace:anything .

grace:has_main_type a owl:ObjectProperty ;
    rdfs:label "has main type"@en ;
    rdfs:domain [ a owl:Class ;
            owl:unionOf ( grace:entry grace:person grace:archive grace:source grace:event grace:type grace:place grace:institution grace:date grace:object grace:monetary_value grace:dedication grace:identifier grace:anything ) ] ;
    rdfs:range grace:type ;
    rdfs:subPropertyOf grace:type_object .

grace:person a owl:Class ;
    rdfs:label "person"@en ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:cardinality "1"^^xsd:nonNegativeInteger ;
            owl:onProperty grace:type_of_person ],
        [ a owl:Restriction ;
            owl:cardinality "1"^^xsd:nonNegativeInteger ;
            owl:onProperty grace:has_main_type ],
        grace:anything .

grace:place a owl:Class ;
    rdfs:label "place"@en ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:cardinality "1"^^xsd:nonNegativeInteger ;
            owl:onProperty grace:has_main_type ],
        [ a owl:Restriction ;
            owl:cardinality "1"^^xsd:nonNegativeInteger ;
            owl:onProperty grace:type_of_place ],
        grace:anything .

grace:institution a owl:Class ;
    rdfs:label "institution"@en ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:cardinality "1"^^xsd:nonNegativeInteger ;
            owl:onProperty grace:has_main_type ],
        [ a owl:Restriction ;
            owl:cardinality "1"^^xsd:nonNegativeInteger ;
            owl:onProperty grace:type_of_institution ],
        grace:anything .

grace:anything a owl:Class ;
    rdfs:label "anything"@en ;
    rdfs:comment "any named resource in the knowledge base" .

grace:object a owl:Class ;
    rdfs:label "object"@en ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:cardinality "1"^^xsd:nonNegativeInteger ;
            owl:onProperty grace:patronage_type ],
        [ a owl:Restriction ;
            owl:cardinality "1"^^xsd:nonNegativeInteger ;
            owl:onProperty grace:care_of_souls ],
        [ a owl:Restriction ;
            owl:maxCardinality "1"^^xsd:nonNegativeInteger ;
            owl:onProperty grace:place_information_within_church_name ],
        [ a owl:Restriction ;
            owl:maxCardinality "1"^^xsd:nonNegativeInteger ;
            owl:onProperty grace:residence ],
        [ a owl:Restriction ;
            owl:cardinality "1"^^xsd:nonNegativeInteger ;
            owl:onProperty grace:type_of_object ],
        [ a owl:Restriction ;
            owl:maxCardinality "1"^^xsd:nonNegativeInteger ;
            owl:onProperty grace:in_deanery ],
        [ a owl:Restriction ;
            owl:cardinality "1"^^xsd:nonNegativeInteger ;
            owl:onProperty grace:has_main_type ],
        [ a owl:Restriction ;
            owl:maxCardinality "1"^^xsd:nonNegativeInteger ;
            owl:onProperty grace:regular_benefice ],
        grace:anything .

grace:type a owl:Class ;
    rdfs:label "type"@en ;
    rdfs:subClassOf grace:anything .

grace:event a owl:Class ;
    rdfs:label "event"@en ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:cardinality "1"^^xsd:nonNegativeInteger ;
            owl:onProperty grace:type_of_event ],
        [ a owl:Restriction ;
            owl:cardinality "1"^^xsd:nonNegativeInteger ;
            owl:onProperty grace:place_of_event ],
        [ a owl:Restriction ;
            owl:cardinality "1"^^xsd:nonNegativeInteger ;
            owl:onProperty grace:expedition_date ],
        [ a owl:Restriction ;
            owl:cardinality "1"^^xsd:nonNegativeInteger ;
            owl:onProperty grace:event_date ],
        [ a owl:Restriction ;
            owl:cardinality "1"^^xsd:nonNegativeInteger ;
            owl:onProperty grace:has_main_type ],
        grace:anything .
