grace: a owl:Ontology ;
    rdfs:label "Graceful17 Ontology"@en ;
    dc:title "Graceful17 Ontology"@en ;
    dcterms:created "2025-10-22T23:41:18"@en ;
    dcterms:creator "Bruno Boute"@en,
        "Christoph Sander"@en ;
    dcterms:license <http://creativecommons.org/licenses/by/4.0/> ;
    ns1:status <http://purl.org/ontology/bibo/status/draft> ;
    vann:preferredNamespacePrefix "grace"@en ;
    vann:preferredNamespaceUri "https://w3id.org/grace/ontology/" ;
    owl:backwardCompatibleWith grace:1.0.0 ;
    owl:imports <http://www.opengis.net/ont/geosparql> ;
    owl:priorVersion grace:1.0.2 ;
    owl:versionIRI grace:1.0.3 ;
    prov:generatedAtTime "2025-10-22T23:41:18"^^xsd:dateTime ;
    schema:citation "Sander, Christoph; Boute, Bruno. (2025). The Graceful17 Ontology, https://w3id.org/g17/ontology (Version 1.0.3)"@en .

grace:abbey a owl:Class ;
    rdfs:label "Abbey"@en ;
    rdfs:comment "A type of church that houses a community of monks or nuns under the authority of an abbot or abbess, typically secluded and dedicated to monastic life." ;
    rdfs:subClassOf grace:seat_of_worship .

grace:abbreviated_name a owl:DatatypeProperty ;
    rdfs:label "abbreviated name"@en ;
    rdfs:domain grace:archive ;
    rdfs:range xsd:string ;
    rdfs:subPropertyOf grace:archive_literal .

grace:absolution a owl:Class ;
    rdfs:label "absolution"@en ;
    rdfs:comment "An ecclesiastical privilege granted by the Church authority, allowing a deviation from the usual canonical laws or rules for specific cases, often used to address unique circumstances or hardships." ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:cardinality "1"^^xsd:nonNegativeInteger ;
            owl:onProperty grace:has_main_type ],
        grace:immaterial_object .

grace:address a owl:DatatypeProperty ;
    rdfs:label "address"@en ;
    rdfs:domain [ a owl:Class ;
            owl:unionOf ( grace:archive grace:place ) ] ;
    rdfs:range xsd:string ;
    rdfs:subPropertyOf grace:place_literal .

grace:affiliated_to_institution a owl:ObjectProperty ;
    rdfs:label "affiliated to institution"@en ;
    rdfs:domain [ a owl:Class ;
            owl:unionOf ( grace:person grace:biographical_event ) ] ;
    rdfs:range grace:institution ;
    rdfs:subPropertyOf grace:pertains_to .

grace:affiliation a owl:Class ;
    rdfs:label "affiliation"@en ;
    rdfs:subClassOf grace:career .

grace:aka a owl:DatatypeProperty ;
    rdfs:label "aka"@en ;
    rdfs:domain grace:anything ;
    rdfs:range xsd:string ;
    rdfs:subPropertyOf grace:appellation_literal .

grace:also_called a owl:DatatypeProperty ;
    rdfs:label "also called"@en ;
    rdfs:domain grace:anything ;
    rdfs:range xsd:string ;
    rdfs:subPropertyOf grace:appellation_literal .

grace:apostolic_absolution a owl:Class ;
    rdfs:label "apostolic absolution"@en ;
    rdfs:subClassOf grace:papal_grace .

grace:apostolic_dispensation a owl:Class ;
    rdfs:label "apostolic dispensation"@en ;
    rdfs:comment "An apostolic dispensation is a special exemption granted directly by the Pope or the Holy See, allowing a deviation from a Church law or regulation in exceptional circumstances. This type of dispensation is typically reserved for significant matters, such as resolving impediments to marriage, relaxing canonical obligations, or addressing unique pastoral needs" ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:cardinality "1"^^xsd:nonNegativeInteger ;
            owl:onProperty grace:has_main_type ],
        grace:papal_grace .

grace:apostolic_provision a owl:Class ;
    rdfs:label "apostolic provision"@en ;
    rdfs:comment "A formal ecclesiastical decree issued by the Pope, governing the provision of ecclesiastical offices and benefits, often in response to specific events or needs within the Church." ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:cardinality "1"^^xsd:nonNegativeInteger ;
            owl:onProperty grace:during_sede_vancate ],
        [ a owl:Restriction ;
            owl:cardinality "1"^^xsd:nonNegativeInteger ;
            owl:onProperty grace:deceased_in_rome ],
        [ a owl:Restriction ;
            owl:maxCardinality "1"^^xsd:nonNegativeInteger ;
            owl:onProperty grace:provision_subform ],
        [ a owl:Restriction ;
            owl:cardinality "1"^^xsd:nonNegativeInteger ;
            owl:onProperty grace:provision_type ],
        [ a owl:Restriction ;
            owl:cardinality "1"^^xsd:nonNegativeInteger ;
            owl:onProperty grace:source_subregister ],
        [ a owl:Restriction ;
            owl:maxCardinality "1"^^xsd:nonNegativeInteger ;
            owl:onProperty grace:vacancy_date ],
        [ a owl:Restriction ;
            owl:cardinality "1"^^xsd:nonNegativeInteger ;
            owl:onProperty grace:has_main_type ],
        [ a owl:Restriction ;
            owl:cardinality "1"^^xsd:nonNegativeInteger ;
            owl:onProperty grace:event_circumstances ],
        grace:papal_grace .

grace:apostolic_supplication a owl:Class ;
    rdfs:label "apostolic supplication"@en ;
    rdfs:comment "An apostolic supplication is a special exemption granted directly by the Pope or the Holy See, allowing a deviation from a Church law or regulation in exceptional circumstances. This type of supplication is typically reserved for significant matters, such as resolving impediments to marriage, relaxing canonical obligations, or addressing unique pastoral needs" ;
    rdfs:subClassOf grace:papal_grace .

grace:archdeanery a owl:Class ;
    rdfs:label "archdeanery"@en ;
    rdfs:comment "An archdeanery is a larger or more prominent division within a diocese, typically encompassing multiple deaneries. It may be overseen by an archdean, who has a role similar to a dean but with broader responsibilities." ;
    rdfs:subClassOf grace:institution .

grace:archdiocese a owl:Class ;
    rdfs:label "archdiocese"@en ;
    rdfs:comment "The primary diocese of an ecclesiastical province, led by an archbishop who oversees the suffragan dioceses within the province." ;
    rdfs:subClassOf grace:diocese .

grace:archive_name a owl:DatatypeProperty ;
    rdfs:label "archive name"@en ;
    rdfs:domain grace:archive ;
    rdfs:range xsd:string ;
    rdfs:subPropertyOf grace:archive_literal .

grace:authority_by_institution a owl:ObjectProperty ;
    rdfs:label "authority by institution"@en ;
    rdfs:comment "an institution being in charge or acting as authority" ;
    rdfs:domain grace:event ;
    rdfs:range grace:institution ;
    rdfs:subPropertyOf grace:in_capacity_of .

grace:basilica a owl:Class ;
    rdfs:label "Basilica"@en ;
    rdfs:comment "A church granted ceremonial privileges by the Pope, often distinguished by its architectural grandeur and historical significance, serving both liturgical and administrative roles." ;
    rdfs:subClassOf grace:seat_of_worship .

grace:benefice a owl:Class ;
    rdfs:label "benefice"@en ;
    rdfs:comment "A right or title granted to an ecclesiastical position, allowing the holder to receive income or benefits in exchange for performing religious duties." ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:maxCardinality "1"^^xsd:nonNegativeInteger ;
            owl:onProperty grace:in_vicariate ],
        [ a owl:Restriction ;
            owl:cardinality "1"^^xsd:nonNegativeInteger ;
            owl:onProperty grace:in_diocese ],
        [ a owl:Restriction ;
            owl:minCardinality "1"^^xsd:nonNegativeInteger ;
            owl:onProperty grace:benefice_category ],
        [ a owl:Restriction ;
            owl:maxCardinality "1"^^xsd:nonNegativeInteger ;
            owl:onProperty grace:disambiguation_of_a_location_of_an_institution ],
        [ a owl:Restriction ;
            owl:cardinality "1"^^xsd:nonNegativeInteger ;
            owl:onProperty grace:has_main_type ],
        [ a owl:Restriction ;
            owl:maxCardinality "1"^^xsd:nonNegativeInteger ;
            owl:onProperty grace:church_category ],
        [ a owl:Restriction ;
            owl:maxCardinality "1"^^xsd:nonNegativeInteger ;
            owl:onProperty grace:religious_order ],
        grace:immaterial_object .

grace:benefice_taxation a owl:ObjectProperty ;
    rdfs:label "benefice taxation"@en ;
    rdfs:comment "The assessment or assignment of financial value to a benefice, indicating the income or revenue it generates, typically for taxation or financial management purposes within the church." ;
    rdfs:domain grace:event ;
    rdfs:range grace:monetary_value ;
    rdfs:subPropertyOf grace:has_monetary_value .

grace:benefice_title a owl:ObjectProperty ;
    rdfs:label "benefice title"@en ;
    rdfs:comment "A specific ecclesiastical name or title dedicated to a benefice, often reflecting its patron saint, founding figure, or primary function within the church community." ;
    rdfs:domain [ a owl:Class ;
            owl:unionOf ( grace:institution grace:object ) ] ;
    rdfs:range grace:dedication ;
    rdfs:subPropertyOf grace:dedicated_to .

grace:biography_event a owl:ObjectProperty ;
    rdfs:label "biography event"@en ;
    rdfs:domain grace:person ;
    rdfs:range grace:biographical_event ;
    rdfs:subPropertyOf grace:participates_in ;
    owl:inverseOf grace:biography_subject .

grace:birth a owl:Class ;
    rdfs:label "birth"@en ;
    rdfs:subClassOf grace:biographical_event .

grace:birth_place a owl:ObjectProperty ;
    rdfs:label "birth place"@en ;
    rdfs:comment "The specific place where an individual was born, important for legal, genealogical, or historical records." ;
    rdfs:domain grace:person ;
    rdfs:range grace:place ;
    rdfs:subPropertyOf grace:related_to_place .

grace:broader a owl:TransitiveProperty ;
    rdfs:label "broader"@en ;
    rdfs:domain grace:type ;
    rdfs:range grace:type ;
    rdfs:subPropertyOf grace:type_object .

grace:called a owl:DatatypeProperty ;
    rdfs:label "called"@en ;
    rdfs:comment "A primary name of a resource" ;
    rdfs:domain grace:anything ;
    rdfs:range xsd:string ;
    rdfs:subPropertyOf grace:appellation_literal .

grace:cantors_house a owl:Class ;
    rdfs:label "Cantor's house"@en ;
    rdfs:comment "A cantor’s house historically refers to the residence of a cantor, a person responsible for leading liturgical music or chanting in a church, synagogue, or other religious setting. It could also serve as a place for rehearsing or organizing musical activities related to worship." ;
    rdfs:subClassOf grace:seat_of_worship .

grace:cardinal_with_duties a owl:ObjectProperty ;
    rdfs:label "cardinal with duties"@en ;
    rdfs:comment "A cardinal assigned specific responsibilities, often administrative or liturgical, reflecting their integral role in the governance of the Church and their close collaboration with the Pope." ;
    rdfs:domain grace:event ;
    rdfs:range grace:person ;
    rdfs:subPropertyOf grace:has_participant .

grace:cardinal_with_indult a owl:ObjectProperty ;
    rdfs:label "cardinal with indult"@en ;
    rdfs:comment "A cardinal who has been granted an indult, which is a special permission or privilege, typically for a specific purpose outside the usual norms of ecclesiastical law." ;
    rdfs:domain grace:event ;
    rdfs:range grace:person ;
    rdfs:subPropertyOf grace:has_participant .

grace:cathedral a owl:Class ;
    rdfs:label "cathedral"@en ;
    rdfs:comment "The principal church of a diocese, housing the bishop's official seat or cathedra, central to diocesan liturgy and administration." ;
    rdfs:subClassOf grace:seat_of_worship .

grace:chapel a owl:Class ;
    rdfs:label "Chapel"@en ;
    rdfs:comment "A small religious building, often located on private grounds or within an institution, used for worship and prayer, typically serving a specific group or community." ;
    rdfs:subClassOf grace:seat_of_worship .

grace:church a owl:Class ;
    rdfs:label "church"@en ;
    rdfs:comment "A building used for public Christian worship, typically smaller and less formal than a cathedral, serving the local community's spiritual needs." ;
    rdfs:subClassOf grace:seat_of_worship .

grace:church_title a owl:ObjectProperty ;
    rdfs:label "church title"@en ;
    rdfs:comment "A specific name or title given to a church, often reflecting its dedication to a saint, historical significance, or geographical location." ;
    rdfs:domain [ a owl:Class ;
            owl:unionOf ( grace:institution grace:object ) ] ;
    rdfs:range grace:dedication ;
    rdfs:subPropertyOf grace:dedicated_to .

grace:coadjutary a owl:Class ;
    rdfs:label "coadjutary"@en ;
    rdfs:comment "An auxiliary or assisting role within the church, where the incumbent supports another cleric in their duties, often due to age or health reasons." ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:cardinality "1"^^xsd:nonNegativeInteger ;
            owl:onProperty grace:has_main_type ],
        grace:immaterial_object .

grace:collegiate_church a owl:Class ;
    rdfs:label "Collegiate church"@en ;
    rdfs:comment "A church endowed with a corporate body of clergy organized as a collegiate body, typically serving a larger community with a focus on liturgical services and choral music." ;
    rdfs:subClassOf grace:seat_of_worship .

grace:commenda a owl:Class ;
    rdfs:label "commenda"@en ;
    rdfs:comment "A temporary ecclesiastical assignment where a cleric holds an office and administers its revenues, often without the obligation of residency or regular duties." ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:cardinality "1"^^xsd:nonNegativeInteger ;
            owl:onProperty grace:has_main_type ],
        grace:immaterial_object .

grace:competitor a owl:ObjectProperty ;
    rdfs:label "competitor"@en ;
    rdfs:comment "A term used to describe someone who is in competition or a candidate for a benefice." ;
    rdfs:domain grace:event ;
    rdfs:range grace:person ;
    rdfs:subPropertyOf grace:has_participant .

grace:confidence a owl:ObjectProperty ;
    rdfs:label "confidence"@en ;
    rdfs:domain grace:description ;
    rdfs:range grace:type .

grace:contains_custom a owl:ObjectProperty ;
    rdfs:label "contains custom"@en ;
    rdfs:domain grace:place ;
    rdfs:range grace:place ;
    rdfs:subPropertyOf grace:place_object ;
    owl:inverseOf grace:falls_within_custom .

grace:context_explanation a owl:DatatypeProperty ;
    rdfs:label "context explanation"@en ;
    rdfs:domain grace:date ;
    rdfs:range xsd:string ;
    rdfs:subPropertyOf grace:date_literal .

grace:continent a owl:Class ;
    rdfs:label "continent"@en ;
    rdfs:comment "A large geographical division of the world, categorized in ecclesiastical terms for administrative and missionary purposes." ;
    rdfs:subClassOf grace:place .

grace:contributor_name a owl:DatatypeProperty ;
    rdfs:label "contributor name"@en ;
    rdfs:domain grace:contributor ;
    rdfs:range xsd:string ;
    rdfs:subPropertyOf grace:contributor_literal .

grace:cool_id a owl:DatatypeProperty ;
    rdfs:label "cool id"@en ;
    rdfs:domain grace:anything ;
    rdfs:range xsd:string ;
    rdfs:subPropertyOf grace:identifier_literal .

grace:correspondance a owl:Class ;
    rdfs:label "correspondance"@en ;
    rdfs:subClassOf grace:textual_source .

grace:credit a owl:Class ;
    rdfs:label "credit"@en ;
    rdfs:subClassOf grace:anything .

grace:currency a owl:ObjectProperty ;
    rdfs:label "currency"@en ;
    rdfs:comment "The currency of a monetary value" ;
    rdfs:domain grace:monetary_value ;
    rdfs:range grace:type .

grace:current_possessor a owl:ObjectProperty ;
    rdfs:label "current possessor"@en ;
    rdfs:comment "A term used to describe an individual who currently holds a benefice or position, relevant in administrative, legal, and ecclesiastical contexts." ;
    rdfs:domain grace:event ;
    rdfs:range grace:person ;
    rdfs:subPropertyOf grace:has_participant .

grace:custom_region a owl:Class ;
    rdfs:label "custom region"@en ;
    rdfs:comment "A specific area defined or customized for particular administrative, ecclesiastical, or geographical purposes, often tailored to local needs or historical boundaries." ;
    rdfs:subClassOf grace:place .

grace:custom_subregion a owl:Class ;
    rdfs:label "custom sub-region"@en ;
    rdfs:comment "A smaller, specific division within a custom region, also defined according to particular administrative, geographical, or ecclesiastical requirements." ;
    rdfs:subClassOf grace:place .

grace:date_attribute a owl:ObjectProperty ;
    rdfs:label "date attribute"@en ;
    rdfs:comment "Additional attributes for a date that provide details beyond the encoding as date period" ;
    rdfs:domain grace:date ;
    rdfs:range grace:type ;
    rdfs:subPropertyOf grace:type_object .

grace:deaconry a owl:Class ;
    rdfs:label "Deaconry"@en ;
    rdfs:comment "A church benefice specifically dedicated to a deacon, emphasizing their liturgical and community service roles within the church's structure." ;
    rdfs:subClassOf grace:seat_of_worship .

grace:deanery a owl:Class ;
    rdfs:label "deanery"@en ;
    rdfs:comment "A deanery is a grouping of neighboring parishes within a diocese, overseen by a dean. It facilitates collaboration among parishes and supports the bishop in administering the diocese." ;
    rdfs:subClassOf grace:institution .

grace:death a owl:Class ;
    rdfs:label "death"@en ;
    rdfs:subClassOf grace:biographical_event .

grace:decided_by a owl:ObjectProperty ;
    rdfs:label "decided by"@en ;
    rdfs:comment "A term used to describe someone who has received a benefit, office, or provision from an ecclesiastical authority." ;
    rdfs:domain grace:event ;
    rdfs:range grace:person ;
    rdfs:subPropertyOf grace:has_participant .

grace:dedication_of a owl:ObjectProperty ;
    rdfs:label "dedication of"@en ;
    rdfs:domain grace:dedication ;
    rdfs:range grace:institution ;
    rdfs:subPropertyOf grace:institution_object .

grace:dedication_scope a owl:ObjectProperty ;
    rdfs:label "dedication scope"@en ;
    rdfs:comment "The entity or compound this dedication extends to" ;
    rdfs:domain grace:dedication ;
    rdfs:range grace:type .

grace:described_by a owl:ObjectProperty ;
    rdfs:label "described by"@en ;
    rdfs:domain [ a owl:Class ;
            owl:unionOf ( grace:event grace:object ) ] ;
    rdfs:range grace:description ;
    rdfs:subPropertyOf grace:refers_to .

grace:dimensions a owl:DatatypeProperty ;
    rdfs:label "dimensions"@en ;
    rdfs:domain grace:source ;
    rdfs:range xsd:decimal ;
    rdfs:subPropertyOf grace:source_literal .

grace:dispensation a owl:Class ;
    rdfs:label "dispensation"@en ;
    rdfs:comment "An ecclesiastical privilege granted by the Church authority, allowing a deviation from the usual canonical laws or rules for specific cases, often used to address unique circumstances or hardships." ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:cardinality "1"^^xsd:nonNegativeInteger ;
            owl:onProperty grace:has_main_type ],
        grace:immaterial_object .

grace:earthworks a owl:ObjectProperty ;
    rdfs:label "EarthWorks"@en ;
    rdfs:comment "Polygons of the boundaries of ca. 750 dioceses across Latin Christendom as they appeared between 1200-1500. Rather than corresponding to any particular year, the map is an amalgam that displays all Latin dioceses that existed at some point during this period. The map excludes dioceses in the Latin East and in parts of eastern Europe whose boundaries are unknown (or were never established)." ;
    rdfs:domain grace:anything ;
    rdfs:range xsd:anyURI ;
    rdfs:subPropertyOf grace:lod .

grace:ecclesiastical_province a owl:Class ;
    rdfs:label "ecclesiastical province"@en ;
    rdfs:comment "A major administrative division within the church, encompassing several dioceses and governed by a metropolitan archbishop, coordinating regional church affairs." ;
    rdfs:subClassOf grace:institution .

grace:ended a owl:DatatypeProperty ;
    rdfs:label "ended"@en ;
    rdfs:domain grace:date ;
    rdfs:range xsd:date ;
    rdfs:subPropertyOf grace:date_literal .

grace:episcopal_dispensation a owl:Class ;
    rdfs:label "episcopal dispensation"@en ;
    rdfs:subClassOf grace:episcopal_grace .

grace:episcopal_provision a owl:Class ;
    rdfs:label "episcopal provision"@en ;
    rdfs:comment "A formal ecclesiastical decree issued by the Pope, governing the provision of ecclesiastical offices and benefits, often in response to specific events or needs within the Church." ;
    rdfs:subClassOf grace:episcopal_grace .

grace:event_described_by_entity a owl:ObjectProperty ;
    rdfs:label "event described by entity"@en ;
    rdfs:domain grace:event ;
    rdfs:range grace:anything ;
    rdfs:subPropertyOf grace:refers_to .

grace:event_related_to_event a owl:SymmetricProperty ;
    rdfs:label "event related to event"@en ;
    rdfs:domain grace:event ;
    rdfs:range grace:event ;
    rdfs:subPropertyOf grace:event_object .

grace:example a owl:DatatypeProperty ;
    rdfs:label "example"@en ;
    rdfs:domain grace:type ;
    rdfs:range xsd:string ;
    rdfs:subPropertyOf grace:type_literal .

grace:executor a owl:ObjectProperty ;
    rdfs:label "executor"@en ;
    rdfs:comment "An official appointed to ensure the execution of ecclesiastical laws and decisions, often tasked with overseeing adherence to canonical directives in specific contexts or situations." ;
    rdfs:domain grace:event ;
    rdfs:range grace:person ;
    rdfs:subPropertyOf grace:has_participant .

grace:falls_within a owl:ObjectProperty ;
    rdfs:label "falls within"@en ;
    rdfs:domain grace:place ;
    rdfs:range grace:place ;
    rdfs:subPropertyOf grace:place_object ;
    owl:inverseOf grace:contains .

grace:falls_within_calculated a owl:ObjectProperty ;
    rdfs:label "falls within calculated"@en ;
    rdfs:domain grace:place ;
    rdfs:range grace:place ;
    rdfs:subPropertyOf grace:place_object .

grace:family_name a owl:DatatypeProperty ;
    rdfs:label "family name"@en ;
    rdfs:domain grace:person ;
    rdfs:range xsd:string ;
    rdfs:subPropertyOf grace:person_literal .

grace:fixed_amount a owl:DatatypeProperty ;
    rdfs:label "fixed amount"@en ;
    rdfs:domain grace:monetary_value ;
    rdfs:range xsd:decimal ;
    rdfs:subPropertyOf grace:money_literal .

grace:floating_amount a owl:DatatypeProperty ;
    rdfs:label "floating amount"@en ;
    rdfs:domain grace:monetary_value ;
    rdfs:range xsd:decimal ;
    rdfs:subPropertyOf grace:money_literal .

grace:folio a owl:DatatypeProperty ;
    rdfs:label "folio"@en ;
    rdfs:domain grace:entry ;
    rdfs:range xsd:string ;
    rdfs:subPropertyOf grace:entry_literal .

grace:for_church a owl:ObjectProperty ;
    rdfs:label "for church"@en ;
    rdfs:comment "An object applies to a specific church" ;
    rdfs:domain grace:object ;
    rdfs:range grace:institution ;
    rdfs:subPropertyOf grace:pertains_to .

grace:former_possessor a owl:ObjectProperty ;
    rdfs:label "former possessor"@en ;
    rdfs:comment "A term used to describe an individual who previously held a benefice or position, important in contexts involving inheritance, legal disputes, or historical records." ;
    rdfs:domain grace:event ;
    rdfs:range grace:person ;
    rdfs:subPropertyOf grace:has_participant .

grace:full_name a owl:DatatypeProperty ;
    rdfs:label "full name"@en ;
    rdfs:domain grace:person ;
    rdfs:range xsd:string ;
    rdfs:subPropertyOf grace:person_literal .

grace:full_name_unordered a owl:DatatypeProperty ;
    rdfs:label "full name unordered"@en ;
    rdfs:comment "Starting with first name" ;
    rdfs:domain grace:person ;
    rdfs:range xsd:string ;
    rdfs:subPropertyOf grace:person_literal .

grace:geometry a owl:DatatypeProperty ;
    rdfs:label "geometry"@en ;
    rdfs:domain [ a owl:Class ;
            owl:unionOf ( grace:archive grace:place grace:institution ) ] ;
    rdfs:range geosparql:geoJSONLiteral ;
    rdfs:subPropertyOf grace:place_literal .

grace:given_name a owl:DatatypeProperty ;
    rdfs:label "given name"@en ;
    rdfs:domain grace:person ;
    rdfs:range xsd:string ;
    rdfs:subPropertyOf grace:person_literal .

grace:grace_stakeholder a owl:ObjectProperty ;
    rdfs:label "grace stakeholder"@en ;
    rdfs:comment "participant in an event of papal grace" ;
    rdfs:domain grace:person ;
    rdfs:range grace:papal_grace ;
    rdfs:subPropertyOf grace:participates_in ;
    owl:inverseOf grace:biography_subject .

grace:has_id a owl:DatatypeProperty ;
    rdfs:label "has id"@en ;
    rdfs:domain grace:anything ;
    rdfs:range xsd:string ;
    rdfs:subPropertyOf grace:identifier_literal .

grace:has_option a owl:ObjectProperty ;
    rdfs:label "has option"@en ;
    rdfs:domain [ a owl:Class ;
            owl:unionOf ( grace:event grace:object ) ] ;
    rdfs:range [ a owl:Class ;
            owl:unionOf ( grace:person grace:event grace:type grace:place grace:institution grace:date grace:object grace:monetary_value grace:dedication ) ] ;
    rdfs:subPropertyOf grace:refers_to .

grace:has_territory a owl:ObjectProperty ;
    rdfs:label "has territory"@en ;
    rdfs:domain grace:institution ;
    rdfs:range grace:institution_territory ;
    rdfs:subPropertyOf grace:place_object .

grace:has_top_term a owl:ObjectProperty ;
    rdfs:label "has top term"@en ;
    rdfs:domain grace:type ;
    rdfs:range grace:type ;
    rdfs:subPropertyOf grace:type_object .

grace:hermitage a owl:Class ;
    rdfs:label "Hermitage"@en ;
    rdfs:comment "A religious building. Hermitages are places of contemplation in which clergy or religious laypeople lead a secluded life of prayer in often secluded hermitages and chapels. They are named after hermits, who do not necessarily have to be religious and live in such buildings or in buildings at all. Hermitages can also be huts or small country houses." ;
    rdfs:subClassOf grace:seat_of_worship .

grace:holds_object a owl:ObjectProperty ;
    rdfs:label "holds object"@en ;
    rdfs:comment "returns all objects (directly and indirectly) held by an institution" ;
    rdfs:domain grace:institution ;
    rdfs:range grace:object ;
    rdfs:subPropertyOf grace:object_object .

grace:holds_office a owl:ObjectProperty ;
    rdfs:label "holds office"@en ;
    rdfs:comment "the position or office held by a person in a biographical event" ;
    rdfs:domain grace:biographical_event ;
    rdfs:range grace:type ;
    rdfs:subPropertyOf grace:type_assigned .

grace:hospital a owl:Class ;
    rdfs:label "Hospital"@en ;
    rdfs:comment "A building dedicated to healthcare services under the auspices of the church, often providing care for the sick and needy within a religious framework." ;
    rdfs:subClassOf grace:seat_of_worship .

grace:id_type a owl:ObjectProperty ;
    rdfs:label "id type"@en ;
    rdfs:range grace:type ;
    rdfs:subPropertyOf grace:has_main_type .

grace:identifies a owl:ObjectProperty ;
    rdfs:label "identifies"@en ;
    rdfs:domain grace:description ;
    rdfs:range grace:anything .

grace:ids a owl:ObjectProperty ;
    rdfs:label "ids"@en ;
    rdfs:domain grace:anything ;
    rdfs:range grace:identifier ;
    rdfs:subPropertyOf grace:identifier_object .

grace:in_archive a owl:ObjectProperty ;
    rdfs:label "in archive"@en ;
    rdfs:domain grace:source ;
    rdfs:range grace:archive ;
    rdfs:subPropertyOf grace:archive_object .

grace:in_ecclesiastical_province a owl:ObjectProperty ;
    rdfs:label "in ecclesiastical province"@en ;
    rdfs:comment "A designation for churches or ecclesiastical offices that are part of a larger ecclesiastical province, highlighting broader administrative structures and church governance." ;
    rdfs:domain grace:object ;
    rdfs:range grace:institution ;
    rdfs:subPropertyOf grace:pertains_to .

grace:in_source a owl:ObjectProperty ;
    rdfs:label "in source"@en ;
    rdfs:domain grace:entry ;
    rdfs:range grace:source ;
    rdfs:subPropertyOf grace:source_object .

grace:in_thesaurus a owl:ObjectProperty ;
    rdfs:label "in thesaurus"@en ;
    rdfs:domain grace:type ;
    rdfs:range [ a owl:Class ;
            owl:unionOf ( grace:type grace:thesaurus ) ] ;
    rdfs:subPropertyOf grace:type_object .

grace:indeterminate_suffragan_diocese a owl:Class ;
    rdfs:label "indeterminate suffragan diocese"@en ;
    rdfs:comment "A type of ecclesiastical institution whose precise hierarchical or geographical affiliations are not clearly defined, often within a larger diocesan structure." ;
    rdfs:subClassOf grace:diocese .

grace:indult a owl:Class ;
    rdfs:label "indult"@en ;
    rdfs:comment "An ecclesiastical privilege granted by the Church authority, allowing a deviation from the usual canonical laws or rules for specific cases, often used to address unique circumstances or hardships." ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:cardinality "1"^^xsd:nonNegativeInteger ;
            owl:onProperty grace:has_main_type ],
        grace:immaterial_object .

grace:informal_patron a owl:ObjectProperty ;
    rdfs:label "informal patron"@en ;
    rdfs:comment "A person who exerts influence over the appointment of ecclesiastical positions through personal or political connections, rather than formal legal rights." ;
    rdfs:domain grace:event ;
    rdfs:range grace:person ;
    rdfs:subPropertyOf grace:has_participant .

grace:institution_name a owl:DatatypeProperty ;
    rdfs:label "institution name"@en ;
    rdfs:domain grace:institution ;
    rdfs:range xsd:string ;
    rdfs:subPropertyOf grace:institution_literal .

grace:is_duplicate_of a owl:ObjectProperty ;
    rdfs:label "is duplicate of"@en ;
    rdfs:comment "if an event or object is interpreted as a semantic duplicate of another entity of the same class." ;
    rdfs:domain [ a owl:Class ;
            owl:unionOf ( grace:event grace:object ) ] ;
    rdfs:range [ a owl:Class ;
            owl:unionOf ( grace:event grace:object ) ] ;
    rdfs:subPropertyOf grace:refers_to .

grace:is_regard_of a owl:ObjectProperty ;
    rdfs:label "is regard of"@en ;
    rdfs:domain grace:type ;
    rdfs:range grace:description ;
    owl:inverseOf grace:regards .

grace:judge a owl:ObjectProperty ;
    rdfs:label "judge"@en ;
    rdfs:comment "A person, usually two, who act as judges in the allocation of offices in the diocese and oversee the process. Not mentioned in Roman sources." ;
    rdfs:domain grace:event ;
    rdfs:range grace:person ;
    rdfs:subPropertyOf grace:has_participant .

grace:kinship a owl:SymmetricProperty ;
    rdfs:label "kinship"@en ;
    rdfs:comment "Some person being related to another person by kinship" ;
    rdfs:domain grace:person ;
    rdfs:range grace:person ;
    rdfs:subPropertyOf grace:person_object .

grace:legal_patron a owl:ObjectProperty ;
    rdfs:label "legal patron"@en ;
    rdfs:comment "An individual or institution with the legal authority to appoint or recommend candidates for ecclesiastical offices, typically rooted in historical rights or privileges." ;
    rdfs:domain grace:event ;
    rdfs:range grace:person ;
    rdfs:subPropertyOf grace:has_participant .

grace:letter a owl:Class ;
    rdfs:label "letter"@en ;
    rdfs:subClassOf grace:entry .

grace:level a owl:DatatypeProperty ;
    rdfs:label "level"@en ;
    rdfs:domain grace:type ;
    rdfs:range xsd:integer ;
    rdfs:subPropertyOf grace:type_literal .

grace:locality a owl:Class ;
    rdfs:label "locality"@en ;
    rdfs:comment "A term for a specific area or neighborhood within a larger geographic or administrative region, often used in ecclesiastical contexts to specify church jurisdictions." ;
    rdfs:subClassOf grace:place .

grace:location a owl:Class ;
    rdfs:label "location"@en ;
    rdfs:comment "A general term for any geographical area or position where a specific event or institution is situated, used broadly in ecclesiastical and secular contexts." ;
    rdfs:subClassOf grace:place .

grace:location_of_an_institution a owl:ObjectProperty ;
    rdfs:label "location of an institution"@en ;
    rdfs:comment "The specific place where an institution is based, often important for legal, administrative, or operational purposes." ;
    rdfs:domain grace:object ;
    rdfs:range grace:place ;
    rdfs:subPropertyOf grace:related_to_place .

grace:main_property a owl:Class ;
    rdfs:label "main property"@en ;
    rdfs:subClassOf grace:term .

grace:membership a owl:Class ;
    rdfs:label "membership"@en ;
    rdfs:comment "membership in a religious order or society, e.g." ;
    rdfs:subClassOf grace:career .

grace:mid_date a owl:DatatypeProperty ;
    rdfs:label "mid date"@en ;
    rdfs:domain grace:date ;
    rdfs:range xsd:date ;
    rdfs:subPropertyOf grace:date_literal .

grace:monastery a owl:Class ;
    rdfs:label "Monastery"@en ;
    rdfs:comment "A religious community where monks or nuns live, work, and worship, following the rules of their religious order, often secluded from the general public to maintain a focus on spiritual life." ;
    rdfs:subClassOf grace:seat_of_worship .

grace:monetary_value_normalized a owl:ObjectProperty ;
    rdfs:label "monetary value normalized"@en ;
    rdfs:comment "A normalized representation of a monetary values" ;
    rdfs:domain grace:monetary_value ;
    rdfs:range grace:type ;
    rdfs:subPropertyOf grace:has_main_type .

grace:mystery_of_faith a owl:Class ;
    rdfs:label "mystery of faith"@en ;
    rdfs:comment "A concept or event that is considered a Mystery of Faithy by the Catholic Church" ;
    rdfs:subClassOf grace:dedication .

grace:name_suffix a owl:DatatypeProperty ;
    rdfs:label "name suffix"@en ;
    rdfs:domain grace:person ;
    rdfs:range xsd:string ;
    rdfs:subPropertyOf grace:person_literal .

grace:name_title a owl:DatatypeProperty ;
    rdfs:label "name title"@en ;
    rdfs:domain grace:person ;
    rdfs:range xsd:string ;
    rdfs:subPropertyOf grace:person_literal .

grace:narrative a owl:Class ;
    rdfs:label "narrative"@en ;
    rdfs:subClassOf grace:entry .

grace:narrower a owl:TransitiveProperty ;
    rdfs:label "narrower"@en ;
    rdfs:domain grace:type ;
    rdfs:range grace:type ;
    rdfs:subPropertyOf grace:type_object .

grace:natural_person a owl:Class ;
    rdfs:label "natural person"@en ;
    rdfs:comment "A legally recognized individual, as opposed to a corporate entity, involved in legal or church-related matters." ;
    rdfs:subClassOf grace:person .

grace:object_described_by_entity a owl:ObjectProperty ;
    rdfs:label "object described by entity"@en ;
    rdfs:domain grace:object ;
    rdfs:range grace:anything ;
    rdfs:subPropertyOf grace:refers_to .

grace:object_related_to_event a owl:ObjectProperty ;
    rdfs:label "object related to event"@en ;
    rdfs:domain grace:object ;
    rdfs:range grace:event ;
    rdfs:subPropertyOf grace:event_object ;
    owl:inverseOf grace:event_related_to_object .

grace:object_related_to_object a owl:SymmetricProperty ;
    rdfs:label "object related to object"@en ;
    rdfs:comment "An object that is related to another object, a symmetrical property." ;
    rdfs:domain grace:object ;
    rdfs:range grace:object ;
    rdfs:subPropertyOf grace:object_object .

grace:object_related_to_object_directed a owl:ObjectProperty ;
    rdfs:label "object related to object directed"@en ;
    rdfs:comment "An object that is related to another object, a non-symmetrical property." ;
    rdfs:domain grace:object ;
    rdfs:range grace:object ;
    rdfs:subPropertyOf grace:object_object .

grace:oratory a owl:Class ;
    rdfs:label "Oratory"@en ;
    rdfs:comment "A designated space for prayer and worship, smaller than a church and often privately funded, used for personal devotion or by specific religious communities without the full status of a parish." ;
    rdfs:subClassOf grace:seat_of_worship .

grace:parish a owl:Class ;
    rdfs:label "Parish"@en,
        "parish"@en ;
    rdfs:comment "A parish is a local community of the faithful within a diocese, entrusted to a parish priest for pastoral care, worship, and administration of sacraments.",
        "A primary ecclesiastical unit providing religious services and community support to a localized area, typically led by a parish priest." ;
    rdfs:subClassOf grace:institution .

grace:parish_church a owl:Class ;
    rdfs:label "Parish Church"@en ;
    rdfs:comment "A church serving a local community or parish, often responsible for delivering regular religious services, community engagement, and spiritual guidance to its congregants." ;
    rdfs:subClassOf grace:seat_of_worship .

grace:parish_hall a owl:Class ;
    rdfs:label "Parish hall"@en ;
    rdfs:comment "A building or space associated with a church used for meetings, social activities, and gatherings of the parish community." ;
    rdfs:subClassOf grace:seat_of_worship .

grace:pension a owl:Class ;
    rdfs:label "pension"@en ;
    rdfs:comment "A regular payment made to a cleric or church official, typically as a retirement benefit or for services rendered, not tied to specific duties." ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:cardinality "1"^^xsd:nonNegativeInteger ;
            owl:onProperty grace:has_main_type ],
        grace:immaterial_object .

grace:pension_value a owl:ObjectProperty ;
    rdfs:label "pension value"@en ;
    rdfs:comment "The specific monetary amount assigned as a pension to a cleric or church official, reflecting ongoing financial support typically derived from church revenues." ;
    rdfs:domain grace:event ;
    rdfs:range grace:monetary_value ;
    rdfs:subPropertyOf grace:has_monetary_value .

grace:physical_archive a owl:Class ;
    rdfs:label "physical archive"@en ;
    rdfs:subClassOf grace:archive .

grace:place_name a owl:DatatypeProperty ;
    rdfs:label "place name"@en ;
    rdfs:domain grace:place ;
    rdfs:range xsd:string ;
    rdfs:subPropertyOf grace:place_literal .

grace:position a owl:Class ;
    rdfs:label "position"@en ;
    rdfs:comment "office or occupation held by a person" ;
    rdfs:subClassOf grace:career .

grace:primary_place a owl:ObjectProperty ;
    rdfs:label "primary place"@en ;
    rdfs:domain grace:institution ;
    rdfs:range grace:place ;
    rdfs:subPropertyOf grace:place_object .

grace:priory a owl:Class ;
    rdfs:label "Priory"@en ;
    rdfs:comment "A religious house governed by a prior or prioress, smaller than an abbey but still dedicated to the monastic life of its community members." ;
    rdfs:subClassOf grace:seat_of_worship .

grace:procurator a owl:ObjectProperty ;
    rdfs:label "procurator"@en ;
    rdfs:comment "A representative of the suppliciant/providee who takes over his work in Rome: collecting documents, forwarding them to the next office, usually for a fee and often also (distant) relatives. Not metioned in the Per obitum and Expeditiones registers." ;
    rdfs:domain grace:event ;
    rdfs:range grace:person ;
    rdfs:subPropertyOf grace:has_participant .

grace:ranking a owl:DatatypeProperty ;
    rdfs:label "ranking"@en ;
    rdfs:domain grace:type ;
    rdfs:range xsd:integer ;
    rdfs:subPropertyOf grace:type_literal .

grace:record a owl:Class ;
    rdfs:label "record"@en ;
    rdfs:subClassOf grace:entry .

grace:record_id a owl:DatatypeProperty ;
    rdfs:label "record id"@en ;
    rdfs:comment "An internal integer ID" ;
    rdfs:domain grace:anything ;
    rdfs:range xsd:positiveInteger ;
    rdfs:subPropertyOf grace:identifier_literal .

grace:reference_date a owl:DatatypeProperty ;
    rdfs:label "reference date"@en ;
    rdfs:domain grace:date ;
    rdfs:range xsd:date ;
    rdfs:subPropertyOf grace:date_literal .

grace:region a owl:Class ;
    rdfs:label "region"@en ;
    rdfs:comment "A general term for a larger geographic or administrative area, commonly used in both secular and ecclesiastical contexts to organize territories and manage affairs." ;
    rdfs:subClassOf grace:place .

grace:related_institution a owl:ObjectProperty ;
    rdfs:label "related institution"@en ;
    rdfs:comment "An institution that is related to a person." ;
    rdfs:domain grace:biographical_event ;
    rdfs:range grace:institution ;
    rdfs:subPropertyOf grace:pertains_to .

grace:related_to_saint a owl:ObjectProperty ;
    rdfs:label "related to saint"@en ;
    rdfs:domain grace:dedication ;
    rdfs:range grace:dedication ;
    rdfs:subPropertyOf grace:dedication_object .

grace:relative_to a owl:DatatypeProperty ;
    rdfs:label "relative to"@en ;
    rdfs:domain grace:date ;
    rdfs:range xsd:date ;
    rdfs:subPropertyOf grace:date_literal .

grace:saint a owl:Class ;
    rdfs:label "saint"@en ;
    rdfs:comment "An individual saint of the Catholic Church" ;
    rdfs:subClassOf grace:dedication .

grace:schema_name a owl:DatatypeProperty ;
    rdfs:label "schema name"@en ;
    rdfs:domain grace:schema ;
    rdfs:range xsd:string ;
    rdfs:subPropertyOf grace:schema_literal .

grace:serial_source a owl:Class ;
    rdfs:label "serial source"@en ;
    rdfs:subClassOf grace:textual_source .

grace:shelf_mark a owl:DatatypeProperty ;
    rdfs:label "shelf mark"@en ;
    rdfs:domain grace:source ;
    rdfs:range xsd:string ;
    rdfs:subPropertyOf grace:source_literal .

grace:st_peters_basilica a owl:Class ;
    rdfs:label "St. Peter's Basilica"@en ;
    rdfs:comment "The largest and most renowned basilica in the world, located in Vatican City, serving as a major pilgrimage site and the principal church of the Roman Catholic Church." ;
    rdfs:subClassOf grace:seat_of_worship .

grace:started a owl:DatatypeProperty ;
    rdfs:label "started"@en ;
    rdfs:domain grace:date ;
    rdfs:range xsd:date ;
    rdfs:subPropertyOf grace:date_literal .

grace:subregion a owl:Class ;
    rdfs:label "sub-region"@en ;
    rdfs:comment "A specific geographic division within a larger region, used in ecclesiastical and civil administration to organize local governance and church jurisdictions." ;
    rdfs:subClassOf grace:place .

grace:suffragan_diocese a owl:Class ;
    rdfs:label "suffragan diocese"@en ;
    rdfs:comment "A diocese that is under the jurisdiction of an archdiocese, with its bishop typically reporting to the metropolitan archbishop." ;
    rdfs:subClassOf grace:diocese .

grace:supreme_body_of_government a owl:Class ;
    rdfs:label "supreme body of government"@en ;
    rdfs:comment "The highest administrative or governing body within a certain context, such as a government or ecclesiastical organization, responsible for overarching decision-making and policy." ;
    rdfs:subClassOf grace:institution .

grace:territorial_prelate a owl:Class ;
    rdfs:label "territorial prelate"@en ;
    rdfs:comment "\"nullius\"" ;
    rdfs:subClassOf grace:institution .

grace:testifies_to_event a owl:ObjectProperty ;
    rdfs:label "testifies to event"@en ;
    rdfs:comment "A characteristic of a record or document indicating that it serves as evidence or confirmation of an event, fact, or status." ;
    rdfs:domain grace:entry ;
    rdfs:range grace:event ;
    rdfs:subPropertyOf grace:event_object,
        grace:testifies_to .

grace:testifies_to_object a owl:ObjectProperty ;
    rdfs:label "testifies to object"@en ;
    rdfs:comment "A characteristic of a record or document indicating that it serves as evidence or confirmation of an event, fact, or status." ;
    rdfs:domain grace:entry ;
    rdfs:range grace:object ;
    rdfs:subPropertyOf grace:object_object,
        grace:testifies_to .

grace:title a owl:DatatypeProperty ;
    rdfs:label "title"@en ;
    rdfs:domain grace:dedication ;
    rdfs:range xsd:string ;
    rdfs:subPropertyOf grace:dedication_literal .

grace:top_term_of_thesaurus a owl:ObjectProperty ;
    rdfs:label "top term of thesaurus"@en ;
    rdfs:domain grace:type ;
    rdfs:range grace:thesaurus ;
    rdfs:subPropertyOf grace:type_object .

grace:transcription_preview a owl:DatatypeProperty ;
    rdfs:label "transcription preview"@en ;
    rdfs:domain grace:entry ;
    rdfs:range xsd:string ;
    rdfs:subPropertyOf grace:entry_literal .

grace:type_name a owl:DatatypeProperty ;
    rdfs:label "type name"@en ;
    rdfs:domain grace:type ;
    rdfs:range xsd:string ;
    rdfs:subPropertyOf grace:type_literal .

grace:type_note a owl:DatatypeProperty ;
    rdfs:label "type note"@en ;
    rdfs:domain grace:type ;
    rdfs:range xsd:string ;
    rdfs:subPropertyOf grace:type_literal .

grace:type_of_dedication a owl:ObjectProperty ;
    rdfs:label "type of dedication"@en ;
    rdfs:comment "The primary type of a dedication" ;
    rdfs:domain grace:dedication ;
    rdfs:range grace:type ;
    rdfs:subPropertyOf grace:has_main_type .

grace:type_of_identifier a owl:Class ;
    rdfs:label "type of identifier"@en ;
    rdfs:subClassOf grace:type .

grace:type_of_type a owl:ObjectProperty ;
    rdfs:label "type of type"@en ;
    rdfs:domain grace:type ;
    rdfs:range grace:type ;
    rdfs:subPropertyOf grace:has_main_type .

grace:unidentified_locality a owl:Class ;
    rdfs:label "unidentified locality"@en ;
    rdfs:comment "A designation for an area whose exact identity or specifics are not clearly defined or recorded, often used in ecclesiastical documents for areas with unclear boundaries." ;
    rdfs:subClassOf grace:place .

grace:unidentified_location a owl:Class ;
    rdfs:label "unidentified location"@en ;
    rdfs:comment "A term used when the specific location of an event, institution, or property cannot be identified or confirmed, often appearing in historical records." ;
    rdfs:subClassOf grace:place .

grace:unidentified_natural_person a owl:Class ;
    rdfs:label "unidentified natural person"@en ;
    rdfs:comment "A term used when the identity of an individual involved in ecclesiastical or legal proceedings cannot be determined or is not specified." ;
    rdfs:subClassOf grace:person .

grace:unidentified_region a owl:Class ;
    rdfs:label "unidentified region"@en ;
    rdfs:comment "A term for areas within ecclesiastical records that are not clearly identified or whose exact location remains unspecified, often used in historical or archival contexts." ;
    rdfs:subClassOf grace:place .

grace:unidentified_saint a owl:Class ;
    rdfs:label "unidentified saint"@en ;
    rdfs:comment "An individual saint of the Catholic Church" ;
    rdfs:subClassOf grace:dedication .

grace:unidentified_subregion a owl:Class ;
    rdfs:label "unidentified sub-region"@en ;
    rdfs:comment "A term for a geographic area within ecclesiastical records that is not clearly defined or recognized, reflecting incomplete or vague documentation." ;
    rdfs:subClassOf grace:place .

grace:university_degree a owl:Class ;
    rdfs:label "university degree"@en ;
    rdfs:subClassOf grace:education .

grace:uuid5 a owl:DatatypeProperty ;
    rdfs:label "uuid5"@en ;
    rdfs:comment "A UUID5 generated from the cool IRI" ;
    rdfs:domain grace:anything ;
    rdfs:range xsd:string ;
    rdfs:subPropertyOf grace:identifier_literal .

grace:vacancy_category a owl:ObjectProperty ;
    rdfs:label "vacancy category"@en ;
    rdfs:comment "A classification referring to the reason a church office or benefice is vacant, such as due to resignation, death, or transfer of the previous holder." ;
    rdfs:domain grace:event ;
    rdfs:range grace:type ;
    rdfs:subPropertyOf grace:type_assigned .

grace:vicarage a owl:Class ;
    rdfs:label "Vicarage"@en ;
    rdfs:comment "A church building or establishment where a vicar serves, often provided as part of a benefice that includes living quarters and a stipend." ;
    rdfs:subClassOf grace:seat_of_worship .

grace:vicariate a owl:Class ;
    rdfs:label "vicariate"@en ;
    rdfs:comment "A vicariate is an administrative division within a diocese or a mission territory, entrusted to a vicar. It is established to facilitate governance and pastoral care, especially in large or geographically challenging areas." ;
    rdfs:subClassOf grace:institution .

grace:wikidata a owl:ObjectProperty ;
    rdfs:label "WikiData"@en ;
    rdfs:domain grace:anything ;
    rdfs:range xsd:anyURI ;
    rdfs:subPropertyOf grace:lod .

grace:wkt a owl:DatatypeProperty ;
    rdfs:label "wkt"@en ;
    rdfs:domain [ a owl:Class ;
            owl:unionOf ( grace:archive grace:place grace:institution ) ] ;
    rdfs:range geosparql:wktLiteral ;
    rdfs:subPropertyOf grace:place_literal .

grace:archive_object a owl:ObjectProperty ;
    rdfs:label "archive object"@en ;
    rdfs:domain [ a owl:Class ;
            owl:unionOf ( grace:source grace:anything ) ] ;
    rdfs:range grace:archive ;
    rdfs:subPropertyOf grace:refers_to .

grace:benefice_category a owl:ObjectProperty ;
    rdfs:label "benefice category"@en ;
    rdfs:comment "A classification used to describe the type or status of a church benefice, reflecting its importance, revenue size, or administrative role within the church hierarchy." ;
    rdfs:domain grace:object ;
    rdfs:range grace:type ;
    rdfs:subPropertyOf grace:type_assigned .

grace:care_of_souls a owl:ObjectProperty ;
    rdfs:label "care of souls"@en ;
    rdfs:comment "A term indicating an ecclesiastical duty or office that involves the pastoral care and spiritual welfare of the church's parishioners." ;
    rdfs:domain grace:object ;
    rdfs:range grace:type ;
    rdfs:subPropertyOf grace:type_assigned .

grace:church_category a owl:ObjectProperty ;
    rdfs:label "church category"@en ;
    rdfs:comment "A term used to categorize a church according to its function, size, or liturgical status, such as cathedral, parish church, or chapel." ;
    rdfs:domain grace:object ;
    rdfs:range grace:type ;
    rdfs:subPropertyOf grace:type_assigned .

